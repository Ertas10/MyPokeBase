<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mypokebase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".detalhes.Pokemon_detail" />
        <activity android:name=".detalhes.Item_detail" />
        <activity android:name=".detalhes.Moves_details" />
        <activity android:name=".detalhes.Tipos" />
        <activity android:name=".listas.item_list" />
        <activity android:name=".listas.Pokemon_list" />
        <activity android:name=".listas.Moves_list" />
        <activity android:name=".listas.Tipo_list"></activity>
    </application>

</manifest>