<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mypokebase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".detalhes.PokemonActivity" />
        <activity android:name=".detalhes.ItemActivity" />
        <activity android:name=".detalhes.MovesActivity" />
        <activity android:name=".detalhes.TypesActivity" />
        <activity android:name=".listas.item_list" />
        <activity android:name=".listas.Pokemon_list" />
        <activity android:name=".listas.Moves_list" />
        <activity android:name=".listas.Tipo_list"></activity>
    </application>

</manifest>